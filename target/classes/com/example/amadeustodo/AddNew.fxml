<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="246.0" prefWidth="342.0" styleClass="newBackground" stylesheets="@buttonHover.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.amadeustodo.AddNewController">
    <children>
        <Button fx:id="addTaskButton" layoutX="87.0" layoutY="86.0" mnemonicParsing="false" onAction="#addTaskButtonPressed" prefHeight="29.0" prefWidth="169.0" text="Task">
         <font>
            <Font name="Ink Free" size="19.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
        <Button fx:id="addNoteButton" layoutX="87.0" layoutY="138.0" mnemonicParsing="false" onAction="#addNoteButtonPressed" prefHeight="25.0" prefWidth="169.0" text="Note">
         <font>
            <Font name="Ink Free" size="18.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
        <Label layoutX="134.0" layoutY="32.0" text="Add New">
            <font>
                <Font name="Ink Free" size="18.0" />
            </font>
        </Label>
      <Button fx:id="closeButton" layoutX="328.0" layoutY="2.0" mnemonicParsing="false" onAction="#closeButtonPressed" prefHeight="25.0" prefWidth="24.0" stylesheets="@buttonHover.css" text="X">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
    </children>
</AnchorPane>
